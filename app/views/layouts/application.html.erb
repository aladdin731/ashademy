<!DOCTYPE html>
<html>
  <head>
    <title>Ashademy</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.COURSE_TYPES = <%= raw Course::TYPES %>
      window.githubIcon = "<%= image_url('github-logo.png') %>";
